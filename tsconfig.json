{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./api/dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "strictPropertyInitialization": false
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules"]
  }
  